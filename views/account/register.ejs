<% if (title) { %>
<h1><%- title %></h1>
<% } else {
  res.redirect('/')
} %>
<form>
  <fieldset class="form-container">
    <label for="account_firstname">First Name:</label>
    <input name="account_firstname" id="account_firstname" type="text">
    <label for="account_lastname">Last Name:</label>
    <input name="account_lastname" id="account_lastname" type="text">
    <label for="account_email">E-mail Address:</label>
    <input name="account_email" id="account_email" type="email">
    <label for="account_password">Password:</label>
    <input name="account_password" id="account_password" type="password">
    <%- messages() %>
    <button type="submit">REGISTER</button>
  </fieldset>
</form>